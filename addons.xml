<addons>
<addon id="plugin.program.sonarr" name="Sonarr" version="0.0.1" provider-name="GB160">
	<requires>
		<import addon="xbmc.python" version="2.14.0"/>
		<import addon="script.module.requests" version="2.9.1"/>
	</requires>
	<extension point="xbmc.python.pluginsource" library="addon.py">
		<provides>executable</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<language/>
		<summary lang="en">Sonarr</summary>
		<description lang="en">Sonarr Interface</description>
		<platform>all</platform>
		<license>GNU General Public License v3</license>
		<forum></forum>
		<source></source>
		<email></email>
	</extension>
</addon>

<addon id="plugin.program.domoticzcontrol"
       name="Domoticz Control"
       version="0.0.6"
       provider-name="Chopper Rob">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.script"
             library="default.py">
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Control Domoticz</summary>
    <description lang="en">Control domoticz from Kodi.</description>
    <platform>all</platform>
    <language></language>
    <website>https://www.chopperrob.nl</website>
  </extension>
</addon>

<addon id="repository.gb160.kodi" name="gb160 kodi repo" version="0.0.1" provider-name="gb160">
	<extension point="xbmc.addon.repository" name="gb160 Add-on Repository">
		<info compressed="false">http://raw.github.com/gb160/gb160-kodi-addons/master/addons.xml</info>
		<checksum>http://raw.github.com/gb160/gb160-kodi-addons/master/addons.xml.md5</checksum>
		<datadir zip="true">http://raw.github.com/gb160/gb160-kodi-addons/master/repo/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>gb160 kodi addons</summary>
		<description>gb160 kodi addons</description>
		<disclaimer></disclaimer>
		<platform>all</platform>
	</extension>
</addon>

